{
    "name": "angularjs-ngdoc-generator",
    "displayName": "AngularJs ngDoc Generator",
    "description": "Generate ngDocs for AngularJS modules, components, directives, services and filters.",
    "version": "0.1.0",
    "publisher": "Luis",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.ngDocModule",
        "onCommand:extension.ngDocComponent",
        "onCommand:extension.ngDocController",
        "onCommand:extension.ngDocFunctionCtrl",
        "onCommand:extension.ngDocFilter",
        "onCommand:extension.ngDocService",
        "onCommand:extension.ngDocDirective"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
          "command": "extension.ngDocModule",
          "title": "ngDoc Module"
        }, {
          "command": "extension.ngDocComponent",
          "title": "ngDoc Component"
        }, {
          "command": "extension.ngDocController",
          "title": "ngDoc Controller"
        }, {
          "command": "extension.ngDocFunctionCtrl",
          "title": "ngDoc Controller Function"
        }, {
          "command": "extension.ngDocFilter",
          "title": "ngDoc Filter"
        }, {
          "command": "extension.ngDocService",
          "title": "ngDoc Service"
        }, {
          "command": "extension.ngDocDirective",
          "title": "ngDoc Directive"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/LuisEGR/angularjs-ngdoc-vscode.git"
    }
}
